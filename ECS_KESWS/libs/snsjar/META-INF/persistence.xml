<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ECS-KESWSPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>databaselayer.ecs_kesws.entities.EcsResCdFileMsg</class>
    <class>databaselayer.ecs_kesws.entities.InternalProductcodes</class>
    <class>databaselayer.ecs_kesws.entities.Pricelist</class>
    <class>databaselayer.ecs_kesws.entities.ResPaymentMsg</class>
    <class>databaselayer.ecs_kesws.entities.ResCdFileMsg</class>
    <class>databaselayer.ecs_kesws.entities.MessageTypes</class>
    <class>databaselayer.ecs_kesws.entities.Users</class>
    <class>databaselayer.ecs_kesws.entities.RecErrorMsg</class>
    <class>databaselayer.ecs_kesws.entities.RecPaymentMsg</class>
    <class>databaselayer.ecs_kesws.entities.EcsDocumentTypes</class>
    <class>databaselayer.ecs_kesws.entities.RecCdFileMsg</class>
    <class>databaselayer.ecs_kesws.entities.PricelistInternalProductcodeDocumentMap</class>
    <class>databaselayer.ecs_kesws.entities.TransactionLogs</class>
    <class>databaselayer.ecs_kesws.entities.KeswsInspectors</class>
    <class>databaselayer.ecs_kesws.entities.CdFileDetails</class>
    <class>databaselayer.ecs_kesws.entities.LogTypes</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/ECS_KESWS?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.password" value="ecsapps"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="ecsapps"/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
    </properties>
  </persistence-unit>
</persistence>
